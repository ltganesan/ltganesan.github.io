---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Lalitha's Projects</title>
 <link href="http://ltganesan.github.io/projects/atom.xml" rel="self"/>
 <link href="http://ltganesan.github.io/projects"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ltganesan.github.io/projects</id>
 <author>
   <name>Lalitha T. Ganesan</name>
   <email>ltganesan05@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://ltganesan.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://ltganesan.github.io/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
